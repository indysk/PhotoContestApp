<li class='contestlist__item'>
  <div class="contestlist__item__contents">
    <span class="contestlist__item__infomation_main-title">Infomation</span>
    <%= link_to contest_path(contest), class: 'contestlist__item__infomation_main-contents' do %>
      <p class="contestlist__item__infomation_main-name"><%= contest.name %></p>
    <% end %>

    <span class="contestlist__item__infomation_sub-title"></span>
    <div class="contestlist__item__infomation_sub-contents">
      <p class='contestlist__item__infomation_sub-host'>主催：<%= print_contest_creator(contest) %></p>
    </div>

    <span class="contestlist__item__links-title">Links</span>
    <div class="contestlist__item__links-contents">
      <%= link_to new_contest_photo_path(contest), class: 'contestlist__item__links-link' do %>
        <i class="contestlist__item__links-link-icon fas fa-search-plus"></i>
        <p class="contestlist__item__links-link-value">More</p>
      <% end %>
      <%= link_to contest_path(contest), class: 'contestlist__item__links-link' do %>
        <i class="contestlist__item__links-link-icon fas fa-vote-yea"></i>
        <p class="contestlist__item__links-link-value">Vote</p>
      <% end %>
      <%= link_to contest_path(contest), class: 'contestlist__item__links-link' do %>
        <i class="contestlist__item__links-link-icon fas fa-images"></i>
        <p class="contestlist__item__links-link-value">View</p>
      <% end %>
    </div>
  </div>
</li>
